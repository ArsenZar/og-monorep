{
  "name": "og-orion",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "db:generate": "pnpm --filter @og/database prisma generate",
    "db:push": "pnpm --filter @og/database prisma db push",
    "db:studio": "pnpm --filter @og/database prisma studio",
    "nest": "nest"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "prettier": "^3.7.4",
    "turbo": "^2.8.3",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
